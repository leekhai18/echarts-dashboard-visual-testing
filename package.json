    "@types/node": "^20.11.19",
    "@types/puppeteer": "^5.4.7",
    "@types/testing-library__user-event": "^4.1.1",
    "canvas": "^2.11.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-image-snapshot": "^6.1.0",
    "jest-preset-angular": "^14.0.0",
    "jest-puppeteer": "^11.0.0",
    "puppeteer": "^24.4.0",
    "typescript": "~5.3.3"
